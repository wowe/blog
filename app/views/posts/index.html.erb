<h1>Listing Posts</h1>

<%= link_to "New Post", new_post_path %>

<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
    </tr>
    <% @posts.each do |p| %>
        <tr>
            <td><%= p.title %></td>
            <td><%= p.text %></td>
            <td><%= link_to 'Show', p %></td>
            <td><%= link_to 'Edit', edit_post_path(p) %></td>
        </tr>
    <% end %>
</table>
